    "@types/passport-local": "^1.0.33",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.20.1",
